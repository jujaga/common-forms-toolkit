{
  "name": "common-forms-toolkit-frontend",
  "version": "1.0.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "node --max_old_space_size=6144 node_modules/@vue/cli-service/bin/vue-cli-service.js build",
    "clean": "rm -rf coverage dist",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "rebuild": "npm run clean && npm run build",
    "reinstall": "npm run purge && npm install",
    "serve": "vue-cli-service serve",
    "test": "npm run test:unit",
    "test:unit": "vue-cli-service test:unit --verbose --forceExit --detectOpenHandles --maxWorkers=10",
    "test:unit:short": "vue-cli-service test:unit --forceExit --detectOpenHandles --maxWorkers=10",
    "pretest": "npm run lint",
    "purge": "rm -rf node_modules"
  },
  "dependencies": {
    "@bcgov/bc-sans": "^1.0.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "keycloak-js": "^10.0.2",
    "nprogress": "^0.2.0",
    "validator": "^13.1.1",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vuetify": "^2.3.6",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-router": "^4.4.6",
    "@vue/cli-plugin-unit-jest": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "axios-mock-adapter": "^1.18.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vuetify": "^1.0.0-beta.6",
    "jest-sonar-reporter": "^2.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.6.0"
  },
  "jestSonar": {
    "reportPath": "coverage"
  }
}
